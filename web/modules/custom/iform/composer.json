{
    "name": "drupal/iform",
    "description": "IForm module for Drupal integration with Indicia.",
    "type": "drupal-custom-module",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Indicia-Team/client_helpers.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Indicia-Team/media.git"
        }
	],
    "require": {
		"drupal/core": "^11",
        "indicia/client_helpers": "^9",
        "indicia/media": "^9",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_tabs": "^2.1"
    },
    "extra": {
        "installer-paths": {
            "web/modules/custom/iform/{$name}": ["type:drupal-custom-module"]
        }
    }
}
